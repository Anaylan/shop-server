{
	"name": "json-server-vercel",
	"version": "1.0.0",
	"description": "Deploy JSON Server to Vercel",
	"main": "api/server.js",
	"scripts": {
		"start": "node api/server.js",
		"auth": "json-server-auth --watch db.json --port 8080",
		"prod": "concurrently \"yarn start\" \"yarn auth\""
	},
	"dependencies": {
		"concurrently": "^8.0.1",
		"cors": "^2.8.5",
		"json-server": "^0.17.0",
		"json-server-auth": "^2.1.0"
	}
}
